#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/acorn@8.10.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/acorn@8.10.0/node_modules/acorn/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/acorn@8.10.0/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/node_modules:/mnt/c/Users/agust/OneDrive/node_modules:/mnt/c/Users/agust/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/mlly@1.4.0/node_modules/mlly/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/mlly@1.4.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/acorn@8.10.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/acorn@8.10.0/node_modules/acorn/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/acorn@8.10.0/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/node_modules:/mnt/c/Users/agust/OneDrive/node_modules:/mnt/c/Users/agust/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/mlly@1.4.0/node_modules/mlly/node_modules:/mnt/c/Users/agust/OneDrive/Escritorio/portfolio/node_modules/.pnpm/mlly@1.4.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.10.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.10.0/node_modules/acorn/bin/acorn" "$@"
fi
